# Hardware
ro.hardware=samsungexynos7870
ro.arch=exynos7870
ro.chipname=exynos7870
ro.product.brand=samsung
ro.product.name=m10lte
ro.product.device=m10lte
ro.build.product=m10lte
ro.treble.enabled=true

# controller
sys.usb.controller=13600000.dwc3
persist.sys.usb.config=mtp,adb

# Encryption
ro.adb.secure=0

# Crypto
ro.crypto.fde_algorithm=AES-256-XTS-DISK
ro.crypto.fs_crypto_blkdev=/dev/block/dm-3
ro.crypto.fuse_sdcard=true
ro.crypto.state=encrypted
ro.crypto.type=block
